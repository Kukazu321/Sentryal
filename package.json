{
  "name": "sentryal-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "bootstrap": "npm install --workspaces",
    "dev": "node scripts/dev.js",
    "dev:wrapper": "npx concurrently \"npm -w backend run dev --silent\" \"npm -w frontend run dev --silent\"",
    "dev:alt": "npx concurrently \"npm -w backend run dev --silent\" \"npm -w frontend run dev:3001 --silent\"",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^2.8.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.939.0",
    "@aws-sdk/lib-storage": "^3.939.0",
    "@aws-sdk/s3-request-presigner": "^3.939.0"
  }
}
