# ============================================================================
# QUOTA MANAGEMENT - INTELLIGENT RATE LIMITING
# ============================================================================

# Street View API Quota Limits
GOOGLE_STREET_VIEW_QUOTA_LIMIT=10000
GOOGLE_STREET_VIEW_QUOTA_RESET_DAY=1  # Day of month when quota resets
GOOGLE_STREET_VIEW_RATE_LIMIT_PER_SECOND=10  # Max requests per second

# Geocoding API Quota Limits  
GOOGLE_GEOCODING_QUOTA_LIMIT=10000
GOOGLE_GEOCODING_QUOTA_RESET_DAY=1
GOOGLE_GEOCODING_RATE_LIMIT_PER_SECOND=50

# Emergency Quota Thresholds (Percentage)
QUOTA_WARNING_THRESHOLD=80  # Send warning at 80% usage
QUOTA_CRITICAL_THRESHOLD=95  # Stop requests at 95% usage

# ============================================================================
# INSAR PROCESSING - HYPER-OPTIMIZED PARAMETERS
# ============================================================================

# Coherence Thresholds for Ultra-Precision
INSAR_DEFAULT_COHERENCE=0.85  # Minimum coherence for reliable measurements
INSAR_ULTRA_COHERENCE=0.95    # Ultra-high quality threshold
INSAR_MINIMUM_COHERENCE=0.5   # Absolute minimum for processing

# Spatial Baseline Constraints (meters)
INSAR_MAX_BASELINE=150.0      # Maximum perpendicular baseline
INSAR_OPTIMAL_BASELINE=50.0   # Optimal baseline for best coherence
INSAR_CRITICAL_BASELINE=200.0 # Critical baseline - emergency processing only

# Temporal Processing Windows (days)
INSAR_TEMPORAL_WINDOW=180     # Standard temporal window
INSAR_SHORT_WINDOW=30         # Short-term change detection
INSAR_LONG_WINDOW=365         # Long-term trend analysis

# Advanced Processing Flags
INSAR_ENABLE_ATMOSPHERIC_CORRECTION=true
INSAR_ENABLE_TOPOGRAPHIC_CORRECTION=true
INSAR_ENABLE_IONOSPHERIC_CORRECTION=true
INSAR_ENABLE_PHASE_UNWRAPPING_3D=true

# ============================================================================
# FUSION ALGORITHM - REVOLUTIONARY PARAMETERS
# ============================================================================

# Co-Registration Precision (meters)
FUSION_MAX_DISTANCE=20.0              # Maximum spatial distance for matching
FUSION_OPTIMAL_DISTANCE=5.0           # Optimal distance for best accuracy
FUSION_PRECISION_DISTANCE=1.0         # Ultra-precision matching distance

# Temporal Matching Windows (days)
FUSION_TEMPORAL_WINDOW=180            # Standard temporal matching window
FUSION_PRECISE_TEMPORAL_WINDOW=30     # Precise temporal matching
FUSION_EMERGENCY_TEMPORAL_WINDOW=365  # Emergency fallback window

# Confidence Thresholds (0.0 - 1.0)
FUSION_MIN_CONFIDENCE=0.9             # Minimum fusion confidence
FUSION_ULTRA_CONFIDENCE=0.98          # Ultra-high confidence threshold
FUSION_EMERGENCY_CONFIDENCE=0.7       # Emergency processing threshold

# Advanced Fusion Flags
FUSION_PROBABILISTIC_WEIGHTING=true   # Enable probabilistic weighting
FUSION_ENABLE_MACHINE_LEARNING=true   # Enable ML-enhanced fusion
FUSION_ENABLE_TEMPORAL_INTERPOLATION=true
FUSION_ENABLE_SPATIAL_INTERPOLATION=true
FUSION_ENABLE_UNCERTAINTY_QUANTIFICATION=true

# Quality Control Thresholds
FUSION_MIN_REGISTRATION_QUALITY=0.9   # Minimum registration quality
FUSION_MIN_FUSION_CONFIDENCE=0.95     # Minimum overall fusion confidence
FUSION_MAX_RESIDUAL_ERROR=2.0         # Maximum residual error (mm)

